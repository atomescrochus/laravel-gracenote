{
    "name": "atomescrochus/laravel-gracenote",
    "type": "library",
    "description": "A Laravel 5.* package to search the Gracenote Web API.",
    "keywords": [
        "atomescrochus",
        "laravel-gracenote",
        "gracenote"
    ],
    "homepage": "https://github.com/atomescrochus/laravel-gracenote",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-Philippe Murray",
            "email": "jp@atomescroch.us",
            "homepage": "http://jpmurray.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.0",
        "illuminate/support": "~5.1|~5.2|~5.3|~5.4|~5.5",
        "nategood/httpful": "^0.2.20"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Atomescrochus\\Gracenote\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atomescrochus\\Gracenote\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Atomescrochus\\Gracenote\\GracenoteServiceProvider"
            ],
            "aliases": {
                "GracenoteAPI": "Atomescrochus\\Gracenote\\Facade\\Gracenote"
            }
        }
    }
}
